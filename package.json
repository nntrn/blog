    "@emotion/server": "11.0.0-next.10",
    "@emotion/styled": "11.0.0-next.10",
    "@theme-ui/presets": "^0.3.0",
    "@theme-ui/prism": "^0.3.0",
    "codemirror": "^5.52.0",
    "emotion-theming": "^10.0.27",
    "front-matter": "^3.1.0",
